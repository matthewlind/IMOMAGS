/*
 * FEATURED THUMBS
 * Carrington slideshow
 *
 */

.featured-articles {
  height: 390px;
  position: relative;
  width: 400px !important; // Override featured-thumbs.css
  
  img {
    display: block;
    max-width: 100%;
  }
  .featured-item-pane {
    border: none;
    width: 400px;
  }
  .featured-item-image {
    border: 5px solid black;
    width: 390px;
  }
  .featured-item-description {
    background: black;
    height: 95px;
    text-align: center;
    top: 275px;
    width: 400px;
    
    h2 {
      @include font-stag-bold;
      
      font-size: 24px;
      line-height: 26px;
    }
  }
  #featured-articles-navigator {
    height: 10px;
    position: absolute;
    right: 0;
    top: 380px;
    
    li {
      padding: 0;
      margin: 0 0 0 5px;
      width: 10px;

      a {
        background: #181818;
        border: none;
        float: left;
        height: 10px;
        width: 10px;

        img {
          display: none;
        }
        &:hover {
         background: #888; 
        }
      }
      &.ui-state-active,
      &.ui-tabs-selected {
        background-image: none;
        
        a {
          background: $red;
          cursor: default;
        }
      }
    }
  }
  
}


