/* Uber Menu */
#header #megaMenu {
  height: 78px;
  margin: 0 auto;
  padding-right: 300px;
  position: relative;
  top: -92px;
  width: 680px;
  z-index: 100;
  
  ul.megaMenu {
    background: image-url('mega_menus_bg.png') 0 0 repeat-x;
    margin: 0;
    padding: 0;
    position: relative;
    
    & > .menu-item {
      @include inline-block;
      
      & > a {
        @include font-stag;
        @include inline-block;
        
        background: image-url('icon/arrow_red_down.png') 90% center no-repeat;
        color: white;
        font-size: 28px;
        line-height: 78px;
        margin-right: 20px;
        padding: 0 38px 0 22px;
        
        &:hover {
          background: $red image-url('icon/arrow_white_down.png') 90% center no-repeat;
          color: white;
        }
        .wpmega-link-title {
          line-height: 78px;
        }
      }
      &.megaHover > a {
        background: $red image-url('icon/arrow_white_down.png') 90% center no-repeat;
        color: white;
      }
      &:first-child {
        a {
          padding-left: 0;
        }
      }
    }
  }
  .sub-menu li a {
    margin: 0;
  }
  ul.sub-menu-1 {
    @include box-shadow(rgba(black, 0.35) 2px 2px 4px);
    
    background: white;
    border: none;
    display: none;
    top: 93px;
  }

  /* Default fly-out menu styles */
  li.ss-nav-menu-reg {
    ul.sub-menu-1 {
      padding: 12px;
      
      & > .menu-item {
        @include font-stag;

        font-size: 18px;
        
        a {
          background: none;
          padding: 0;

          &:hover {
            color: $red;
          }
        }
      }
      ul.sub-menu-2 {
        @include box-shadow(none);
        padding: 0;
        
        & > .menu-item {
          @include font-helvetica;
          
          font-size: 15px;
        }
      }
    }
  }

  /* Default mega menu styles */
  li.ss-nav-menu-mega {
    ul.sub-menu-1 {
      left: 0 !important;
      padding: 24px;
      top: 93px;
      width: 932px !important;
      
      & > .menu-item {
        @include font-stag;

        font-size: 21px;
        line-height: 1.5;
        
        a {
          background: none;
          padding: 0;

          &:hover {
            color: $red;
          }
        }
        .wpmega-link-title {
          line-height: 1;
        }
      }
      .menu-item {
        background: none;
        line-height: 1.2;
        margin: 0 16px 0 0;
        width: 192px;
        
        a:hover {
          color: black;
        }
        &.ss-sidebar {
//          margin-right: 0;
          width: 308px;
        }
        .wpmega-widgetarea {
          padding: 0;
          width: 100%;
        }
      }
      ul.sub-menu-2 {
        @include box-shadow(none);
        
        left: 0 !important;
        margin: 0.5em 0 1em 0;
        padding: 0;
        position: relative;
        top: 0 !important;
        width: 100% !important;
        
        & > .menu-item {
          @include font-helvetica;
          
          font-size: 16px;
          
          a {
            color: $gunmetal;
            
            &:hover {
              color: black;
            }
          }
          .wpmega-link-title {
            line-height: 1.8;
          }
        }
      }
    }
  }
  
  /* Reviews */
  li.reviews {
    position: static;
  }
  
  /* Shooting */
  li.shooting {
    position: static;
  }
  
  /* Blogs */
  li.blogs ul.sub-menu-1 {
    width: 400px;
    
    .news-brief,
    .sons-guns-ammo,
    .defend-thyself,
    .history-books,
    .love-competition,
    .zombie-nation {
      background-image: image-url('mm_blogs_sprite.jpg');
    }
    
    & > .menu-item {
      background-color: #E5E5E5;
      background-repeat: no-repeat;
      line-height: 1;
      margin-top: 4px;
      padding: 0;
      
      a {
        background: image-url('mm_blogs_arrow.png') right center no-repeat;
        color: black;
        display: block;
        font-size: 18px;
        height: 80px;
        padding: 0 0 0 20px;
        width: 380px;
      }
      .wpmega-link-title {
        line-height: 80px;
      }
      &:first-child {
        margin: 0;
      }
      &:hover {
        a {
          background-image: image-url('mm_blogs_arrow_hover.png');
          color: white;
        }
      }
      &.news-brief {
       background-position: left top; 
       &:hover {
         background-position: right top;
       }
      }
      &.sons-guns-ammo {
       background-position: left -80px; 
       &:hover {
         background-position: right -80px;
       }
      }
      &.defend-thyself {
       background-position: left -160px; 
       &:hover {
         background-position: right -160px;
       }
      }
      &.history-books {
       background-position: left -240px; 
       &:hover {
         background-position: right -240px;
       }
      }
      &.love-competition {
       background-position: left -320px; 
       &:hover {
         background-position: right -320px;
       }
      }
      &.zombie-nation {
       background-position: left -400px; 
       &:hover {
         background-position: right -400px;
       }
      }
    }
  }
  
  /* Video */
  li.video {
    position: static;
  }
  
  /* The Magazine */
  ul.megaMenu li.magazine {
    position: absolute;
    right: -300px;
    z-index: 120;
    
    & > a {
      background: image-url('icon/arrow_black_down.png') 90% center no-repeat;
      font-size: 20px;
      line-height: 76px;
      padding-right: 37px;
      margin: 0;
    }
    &.megaHover > a {
      background-image: image-url('icon/arrow_black_down.png');
      background-color: white;
      color: black;
    }
    ul.sub-menu-1 {
      left: auto !important;
      right: 0 !important;
      top: 78px;
    }
  }
  
  /* Widgets */
  .wpmega-widgetarea {
    .widget {
      margin: 0 0 26px;
      width: 100%;
      
      li {
        font-size: 14px;
        margin: 0.5em 0;
        
        a {
          color: $gunmetal;
          font-weight: bold;
        }
      }
      &.wpmega-last-child {
        margin-bottom: 10px;
      }
    }
    .widgettitle {
      font-size: 24px;
      margin: 0;
    }
    
    /* Popular Posts Widget */
    .popular-posts {
      .wpp-thumbnail {
        float: left;
        margin-right: 14px;
      }
      .wpp-post-title {
        @include font-stag;
        
        color: $gunmetal;
        font-size: 16px;
        line-height: 1.2;
        
        &:hover {
         color: black; 
        }
      }
      .wpp-excerpt {
        font-size: 13px;
        line-height: 1.5;
      }
    }
    
    /* Subscribe Form Widget */
    .widget_subscribe-form {
      .title {
        display: none;
      }
      .subscribe-row {
        margin-bottom: 5px;
      }
    }
  }
}

