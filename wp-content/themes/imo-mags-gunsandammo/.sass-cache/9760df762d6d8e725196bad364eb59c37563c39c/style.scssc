@import "global";
@import "icon/*.png";
@include all-icon-sprites;


* {
  margin: 0;
  padding: 0;
}
html {
  background: white;
  height: 100%;
  overflow: auto;
}
body {
  background: white;
  color: $darkgray;
  font: 100%/1.5 "Helvetica Nueue", Helvetica, Arial, sans-serif;
  position: relative;
  width: 100%;
  -webkit-font-smoothing: antialiased;
}


/*
 * GLOBAL STYLE
 * default styles for elements that will be used everywhere
 *
 */

h1, h2, h3, h4, h5, h6 {
  @include font-stag-bold;

  color: #252525;
}
h1 {
  font-size: 34px;
  line-height: 36px;
}
h2 { font-size: 24px; }
h3 { font-size: 22px; }
h4 { font-size: 20px; }
h5 { font-size: 18px; }
h6 { font-size: 16px; }
p,ul,ol,dl {
  font-size: 15px;
  margin: 1em 0;
}
p { line-height: 28px; }
a, a:visited {
  color: $red;
  text-decoration: none;
}
a:hover, a:focus {
//  border-bottom: 1px dotted $red;
}
hr {
  border: none;
  border-bottom: 1px solid #D1D1D1;
  margin: 2em 30px 1em;
}

/* Lists - Fancy and otherwise */
ul.fancy,
ol.fancy,
.fancy ul,
.fancy ol {
  list-style: none;
  font-size: 13px;
  padding: 0;
  text-align: center;

  li {
    @include ellipsis;
    
    background: white;
    display: block;
    line-height: 1.2;
    padding: 7px 4px;
    
    &:nth-child(2n + 1) {
      background: #F3F3F3;
    }
    a {
      color: $darkgray;
      font-weight: normal;
      
      &:hover {
        color: $red;
      }
    }
  }
}

ol.fancy,
.fancy ol {
  li {
    border-bottom: 1px solid white;
    min-height: 30px;
    padding-left: 34px;
    position: relative;

    span {
      @include font-stag-bold;

      background: black;
      color: white;
      display: block;
      font-size: 16px;
      height: 30px;
      left: 0;
      position: absolute;
      top: 0;
      width: 30px;
    }
  }
}

/* Fancy Titles */
.fancy {
  text-align: center;

  h4 {
    @include inline-block;

    background: image-url('box_title_bg.png') left center repeat-x;
    padding: 0 17px;
    position: relative;
    text-align: center;
    top: -7px;

    span {
      background: white;
      padding: 0 7px;
      position: relative;
    }
  }
}

/* Call to Action */
a.cta {
  @include font-platin;
  
  color: black;
  font-size: 14px;
  position: relative;
  
  &.light { // reduced contrast version
    color: $gunmetal;
    
    span {
      @include icon-sprite(arrow_gray);
    }
    &:hover {
      color: black;
      
      span {
        @include icon-sprite(arrow_red);
      }
    }
  }
  span {
    @include inline-block;
    @include icon-sprite(arrow_red);
    
    margin-left: 5px;
    position: relative;
    top: -2px;
  }
  &:hover {
    border: none;
    color: $red;
  }
}

/* Buttons */
a.button {
  @include inline-block;
  @include font-stag;
  
  background: $red image-url('button_bg.png') 0 0 repeat-x;
  border: 1px solid #AF0C10;
  color: white;
  font-size: 14px;
  height: 35px;
  line-height: 35px;
  padding: 0 49px 0 42px;
  position: relative;
  text-align: center;
  vertical-align: baseline;
  
  span {
    @include inline-block;
    @include icon-sprite(arrow_white);

    right: 34px;
    position: absolute;
    top: 13px;
  }
  &:hover {
    background: black;
    border-color: black;
  }
}

/* Comments */
a.comment-count {
  @include icon-sprite(comments);
  @include font-stag;
  
  color: white;
  display: block;
  font-size: 12px;
  height: 21px;
  line-height: 21px;
  padding-bottom: 6px;
  text-align: center;
  
  &:hover {
    border: none;
  }
}

/* Widgets */
#sidebar,
#secondary,
.cfct-sidebar,
#carrington-modules .cfct-row-ab-c .cfct-block-c {
  .widget {
    background: image-url('widget_stripe_bg_38.png') 4px 0 repeat-x;
    border-top: 4px solid black;

    .widget-title {
      font-size: 18px;
      height: 72px;
      line-height: 1;
      position: relative;
      text-align: center;

      span {
        background: white;
        position: relative;
        padding: 6px 10px 0;
        top: 26px;
      }
    }
  }
}

/* Slideshow */
.ui-tabs {
  left: 0;
  position: relative;
  text-align: center;
  top: 0;
  
  & > div {
    background: black;
    border: 4px solid black;
    left: 0;
    position: absolute;
    top: 0;
  }
  .ui-tabs-nav {
    list-style: none;
    margin: 0;
    padding: 0;
    position: relative;
    
    li {
      @include inline-block;
      
      a {
        @include inline-block;
        
        background: #181818;
        cursor: pointer;
        height: 10px;
        width: 10px;

        &:hover {
         background: #888; 
        }
      }
      &.ui-tabs-selected a,
      &.ui-tabs-active a {
        background: $red;
        cursor: default;
      } 
    }
  }
  .prev-tab,
  .next-tab {
    @include hide-text;
    
    background: black image-url('ui_tab_prev.png') center center no-repeat;
    display: none;
    height: 60px;
    position: absolute;
    top: 150px;
    width: 60px;
    z-index: 30;
    
    &:hover {
      background-color: $red;
    }
  }
  .prev-tab {
    left: 0;
  }
  .next-tab {
    background-image: image-url('ui_tab_next.png');
    right: 0;
  }
}


/*
 * ENTRY EXCERPT
 * for lists of posts such as archives
 *
 */
 
.entry-excerpt {
  float: left;
  margin-bottom: 50px;
  position: relative;
  width: 100%;

  .entry-img {
    float: left;
    height: 147px;
    margin-right: 22px;
    width: 226px;
  }
  .author-photo {
    left: -8px;
    position: absolute;
    top: -14px;
  }
  .entry-summary {
    float: left;
    line-height: 1;
    width: 358px;
  }
  .entry-category {
    @include font-platin;

    font-size: 14px;
    
    a {
      &:hover {
        border: none;
        color: black;
      }
    }
  }
  .entry-title {
    @include font-stag;

    font-size: 22px;
    line-height: 1.2;
    margin-top: 5px;
    width: 95%;

    a {
      @include inline-block;
      
      color: $gunmetal;

      &:hover {
        border: none;
        color: black;
      }
    }
  }
  a.comment-count {
    position: absolute;
    right: 15px;
    top: 15px;
  }
  p {
    font-size: 13px;
    line-height: 1.5;
  }
}


/*
 * ENTRY EXCERPT
