@import "global";
@import "icon/*.png";
@include all-icon-sprites;


* {
  margin: 0;
  padding: 0;
}
html {
  background: white;
  height: 100%;
  overflow: auto;
}
body {
  background: white;
  color: $darkgray;
  font: 100%/1.5 "Helvetica Nueue", Helvetica, Arial, sans-serif;
  position: relative;
  width: 100%;
  -webkit-font-smoothing: antialiased;
}


/*
 * GLOBAL STYLE
 * default styles for elements that will be used everywhere
 *
 */

h1, h2, h3, h4, h5, h6 {
  @include font-stag-bold;

  color: #252525;
}
h1 {
  font-size: 34px;
  line-height: 36px;
}
h2 { font-size: 24px; }
h3 { font-size: 22px; }
h4 { font-size: 20px; }
h5 { font-size: 18px; }
h6 { font-size: 16px; }
p,ul,ol,dl {
  font-size: 15px;
  margin: 1em 0;
}
p { line-height: 28px; }
a, a:visited {
  color: $red;
  text-decoration: none;
}
a:hover, a:focus {
//  border-bottom: 1px dotted $red;
}
hr {
  border: none;
  border-bottom: 1px solid #D1D1D1;
  margin: 2em 30px 1em;
}

/* Lists - Fancy and otherwise */
ul.fancy, ol.fancy {
  list-style: none;
  font-size: 13px;
  line-height: 30px;
  padding: 0;
  text-align: center;

  li {
    background: white;
    display: block;
    
    &:nth-child(2n + 1) {
      background: #F3F3F3;
    }
    a {
      color: $darkgray;
      font-weight: normal;
      
      &:hover {
        color: $red;
      }
    }
  }
}


ol.fancy {
  li {
    border-bottom: 1px solid white;
    min-height: 30px;
    padding-left: 34px;
    position: relative;

    span {
      @include font-stag-bold;

      background: black;
      color: white;
      display: block;
      font-size: 16px;
      height: 30px;
      left: 0;
      position: absolute;
      top: 0;
      width: 30px;
    }
  }
}

/* Call to Action */
a.cta {
  @include font-platin;
  
  color: black;
  font-size: 14px;
  position: relative;
  
  &.light { // reduced contrast version
    color: $gunmetal;
    
    span {
      @include icon-sprite(arrow_gray);
    }
    &:hover {
      color: black;
      
      span {
        @include icon-sprite(arrow_red);
      }
    }
  }
  span {
    @include inline-block;
    @include icon-sprite(arrow_red);
    
    position: relative;
    top: -2px;
  }
  &:hover {
    border: none;
    color: $red;
  }
}

/* Buttons */
a.button {
  @include inline-block;
  @include font-platin;
  
  background: $red image-url('button_bg.png') 0 0 repeat-x;
  border: 1px solid #AF0C10;
  color: white;
  font-size: 14px;
  height: 35px;
  line-height: 35px;
  padding: 0 49px 0 42px;
  position: relative;
  text-align: center;
  vertical-align: baseline;
  
  span {
    @include inline-block;
    @include icon-sprite(arrow_white);

    right: 34px;
    position: absolute;
    top: 13px;
  }
  &:hover {
    background: black;
    border-color: black;
  }
}

/* Comments */
a.comment-count {
  @include icon-sprite(comments);
  @include font-stag;
  
  color: white;
  display: block;
  font-size: 12px;
  height: 21px;
  line-height: 21px;
  padding-bottom: 6px;
  text-align: center;
  
  &:hover {
    border: none;
  }
}

/* Widgets */
#sidebar,
#secondary,
.cfct-sidebar {
  .widget {
    background: image-url('secondary_section_bg.png') 4px 0 repeat-x;
    border-top: 4px solid black;

    .widget-title {
      font-size: 18px;
      height: 72px;
      line-height: 1;
      position: relative;
      text-align: center;

      span {
        background: white;
        position: relative;
        padding: 6px 10px 0;
        top: 26px;
      }
    }
  }
}


/*
 * ENTRY EXCERPT
 * for lists of posts such as archives
 *
 */
 
.entry-excerpt {
  float: left;
  margin-bottom: 50px;
  position: relative;
  width: 100%;

  .entry-img {
    float: left;
    height: 147px;
    margin-right: 22px;
    width: 226px;
  }
  .author-photo {
    left: -8px;
    position: absolute;
    top: -14px;
  }
  .entry-summary {
    float: left;
    width: 358px;
  }
  .entry-category {
    @include font-platin;

    font-size: 14px;
    a {
      &:hover {
        border: none;
        color: black;
      }
    }
  }
  .entry-title {
    @include font-stag;

    font-size: 22px;
    line-height: 1.2;
    width: 95%;

    a {
      color: $gunmetal;

      &:hover {
        border: none;
        color: black;
      }
    }
  }
  a.comment-count {
    position: absolute;
    right: 15px;
    top: 15px;
  }
  p {
    font-size: 13px;
    line-height: 1.5;
  }
}


/*
 * ENTRY EXCERPT
 * for lists of posts such as archives
 *
 */

.entry-full {}


/*
 * LAYOUT
 * Create a layout with the following elements
 *
 */
 
#header {
  width: 100%;
}
.container {
  @include container;
  position: relative;
}
#main {
  padding: 0 10px; // So the author photos can hang over the edge

  #breadcrumbs {
    @include column(8);
  }
  #sidebar, .cfct-sidebar {
    @include column(4,true);
    float: right;
    margin-left: 16px;
    width: 300px;
  }
  #content, .cfct-block-abc { // may need to apply to home page only
    @include column(8);
    float: left;
  }
  #secondary {
    @include column(12,true);
    
    .widget {
      float: left;
      margin-right: 16px;
      width: 316px;
      
      &:last-child {
        margin-right: 0;
      }
    }
  }
}
#footer {
  #imo-network {
    .mag {
      @include column(3);
      &.last {
        @include column(3,true);
      }
    }
  }
}

/* Carrington Module Layout */
#carrington-modules.col-abc {
  width: 100%;
  .cfct-row {
    float: left;
    margin-bottom: 32px;
    width: 100%;
  }
  .cfct-block-a,
  .cfct-block-b,
  .cfct-block-c {
    @include column(4);
  }
  .cfct-block-ab,
  .cfct-block-bc {
    @include column(8);
  }
  .cfct-block-abc {
    @include column(12);
  }
  .cfct-block-c,
  .cfct-block-bc,
  .cfct-block-abc {
    margin-right: 0;
  }
}

/* Carrington Module Layout within #content */
#content {
  .cfct-row {
    float: left;
    margin-bottom: 32px;
    width: 100%;
  }
  .cfct-block-a,
  .cfct-block-c {
    @include column(3);
  }
  .cfct-block-ab,
  .cfct-block-bc {
    @include column(5);
  }
  .cfct-block-abc {
    @include column(8);
  }
  .cfct-block-c,
  .cfct-block-bc,
  .cfct-block-abc {
    margin-right: 0;
  }
  .cfct-row-a-b-c {
    .cfct-block {
      float: left;
      margin-right: 15px;
      width: 206px;
    }
    .cfct-block-c {
      margin-right: 0;
    }
  }
}


/*
