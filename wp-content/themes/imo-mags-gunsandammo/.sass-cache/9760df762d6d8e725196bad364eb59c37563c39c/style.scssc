      color: #B5B5B5;
      padding: 12px 0;
    }
    .entry-excerpt {
      float: left;
      margin: 0 0 0 15px;
      text-align: center;
      width: 206px;
      
      &:first-child {
        margin: 0;
      }
      a {
        @include inline-block;
      }
    }
    .entry-img {
      height: 126px;
      margin: 0 0 16px;
      width: 206px;
    }
    .entry-title,
    .entry-summary {
      width: 100%;
    }
    .entry-title {
      font-size: 16px;
      a {
        color: black;
        &:hover {
          color: $red;
        }
      }
    }
  }
  #sidebar { // So the sidebar lines up with the breadcrumbs
    position: relative;
//    top: -44px;
  }
} 


/*
 * SIDEBAR
 *
 */
 
#sidebar,
.cfct-sidebar,
#carrington-modules .cfct-row-ab-c .cfct-block-c {
  .widget {
    margin-bottom: 32px;
  }
  .widget_advert-widget {
    background: none;
    border: none;
    
    img {
      display: block;
    }
  }
  .box {
    background: image-url('box_bg.png');
    margin-bottom: 32px;
    padding: 4px;
    text-align: center;
    
    .content_wrapper {
      background: white;
      padding: 18px 12px;
    }
    .box_title {
      @include inline-block;
      
      background: image-url('box_title_bg.png') left center repeat-x;
      padding: 0 17px;
      position: relative;
      top: -7px;
      
      span {
        background: white;
        padding: 0 7px;
        position: relative;
      }
    }
  }
}

/* Search Widget */
.widget_search {
  background: image-url('box_bg.png');
  padding: 4px;
  position: relative;
  text-align: center;

  #searchform {
    background: white;
    padding: 12px;
    position: relative;
  }
  input[type="search"], input.searchfield {
    border: 1px solid #CCC;
    font-size: 13px;
    line-height: 1;
    padding: 10px 16px 9px 6px;
    position: relative;
    width: 244px;
  }
  input[type="submit"] {
    @include hide-text;
    
    background: $red image-url('icon/arrow_white.png') center center no-repeat;
    border: none;
    height: 28px;
    position: absolute;
    right: 16px;
    text-transform: capitalize;
    top: 16px;
    width: 28px;
    
    &:hover {
      background: black image-url('icon/arrow_white.png') center center no-repeat;
    }
  }
}

/* Newsletter Signup Widget */
#newsletter_signup {
  .gform_wrapper {
    padding: 0;
    
    form {
      position: relative;
    }
    
    ul {
      list-style: none;
      margin: 0;
      padding: 0;
      
      li {
        position: relative;
      }
    }
    
    input[type="text"] {
      border: 1px solid #CCC;
      color: #B0B0B0;
      font-size: 13px;
      line-height: 1;
      padding: 10px 16px 9px 6px;
      position: relative;
      width: 244px;
      
      &:focus {
        color: black;
      }
    }
    input[type="submit"] {
      @include hide-text;
      
      background: $red image-url('icon/arrow_white.png') center center no-repeat;
      border: none;
      height: 28px;
      position: absolute;
      right: 4px;
      text-transform: capitalize;
      top: 4px;
      width: 28px;
      
      &:hover {
        background: black image-url('icon/arrow_white.png') center center no-repeat;
      }
    }
  }
}

/* YARPP Widget */
.widget_yarpp_widget {
  @include clearfix;

  .entry-excerpt {
    margin-bottom: 40px;
  }
  .entry-img {
    height: 96px;
    margin-right: 14px;
    width: 140px;
  }
  .entry-summary {
    line-height: 17px;
    width: 146px;
  }
  .entry-title {
    font-size: 16px;
    line-height: 20px;
  }
}

/* Subscribe Form Widget */
.widget_subscribe-form {
  background-position: left bottom !important;
  border-bottom: 1px solid #CCC;
  
  .mag-cover {
    float: left;
    margin: 24px 0 27px;
    
    img {
      display: block;
      width: 106px;
    }
  }
  .title {
    @include font-stag;

    font-size: 24px;
    line-height: 1.1;
    margin: 0 0 27px 125px;
    position: relative;
    top: 54px;
    width: 150px;
  }
  .content {
    clear: both;
  }
  .subscribe-row {
    margin-bottom: 6px;
    width: 100%;
    
    .text {
      color: $gunmetal;
      display: block;
      float: left;
      font-size: 13px;
      padding: 5px 10% 5px 0;
      text-align: right;
      width: 32%;
      
      &.zip {
        clear: left;
      }
    }
    input {
      @include box-shadow(inset rgba(black, 0.1) 0 2px 5px);
      
      border: 1px solid #CCC;
      font-size: 14px;
      padding: 6px 3px;
      width: 55%;
    }
    select {
      margin-bottom: 6px;
    }
  }
  .subscribeButton {
    margin: 32px 0 0;
    text-align: center;
  }
  input[type="submit"] {
    @include inline-block;
    @include font-stag;

    background: $red image-url('button_bg.png') 0 0 repeat-x;
    border: 1px solid #AF0C10;
    color: white;
    font-size: 16px;
    height: 35px;
    line-height: 35px;
    margin-left: 66px;
    padding: 0 20px 0 20px;
    position: relative;
    text-align: center;
    vertical-align: baseline;

    span {
      @include inline-block;
      @include icon-sprite(arrow_white);

      right: 34px;
      position: absolute;
      top: 13px;
    }
    &:hover {
      background: black;
      border-color: black;
    }
  }
}

/* Popular Posts Widget */
#sidebar .popular-posts,
.cfct-sidebar .popular-posts {
  ul {
    list-style: none;
    margin: 0;
    padding: 0;
  }
  li {
    float: left;
    margin-bottom: 40px;
    width: 100%;
  }
  a {
    color: $gunmetal;
    
    &:hover {
      color: black;
    }
  }
  .wpp-thumbnail {
    float: left;
    margin-right: 14px;
  }
  .wpp-post-title {
    @include font-stag;
    
    font-size: 16px;
    line-height: 20px;
  }
}

/* IMO Slider Widget */
.widget_slider-widget {
  position: relative;
  
  .scroll_mask {
    margin-bottom: 16px;
    width: 570px;
  }
  .scroll {
    li {
      margin: 0 12px 0 0;
      font-size: 13px;
      font-weight: bold;
      line-height: 1.3;
      text-align: center;
      
      img {
        display: block;
        margin-bottom: 15px;
      }
      a {
        color: $gunmetal;
        display: block;
        line-height: 1.2;
        
        &:hover {
          color: black;
        }
      }
    }
  }
  .prev, .next {
    @include hide-text;

    background-color: black;
    background-position: center center;
    background-repeat: no-repeat;
    cursor: pointer;
    display: block;
    height: 90px;
    position: absolute;
    top: 0;
    width: 25px;
    
    &:hover {
      background-color: red;
    }
    &.inactive {
      background-color: #E0E0E0;
      cursor: default;
    }
  }
  .prev {
    background-image: image-url('icon/arrow_white_left.png');
    left: 0;
  }
  .next {
    background-image: image-url('icon/arrow_white.png');
    right: 0;
  }
}

/* G&A iPad App Widget */
#sidebar #get_ipad_app {
  background-image: image-url('widget_stripe_bg_78.png');
  background-position: 0 bottom;
  background-repeat: repeat-x;
  border-bottom: 1px solid #CCC;
  padding: 32px 0 0 0;
  text-align: center;
  
  .title {
    @include font-stag;
    
    background: none;
    color: black;
    font-size: 22px;
    line-height: 1.2;
  }
  .cta {
    color: $red;
    
    &:hover {
      color: black;
    }
  }
  .ipad {
    background: image-url('widget_ipad_app.png') center bottom no-repeat;
    height: 161px;
    margin: 24px auto 0;
    width: 244px;
  }
}

/* Channel Widget */
.widget_channel-widget,
#header #megaMenu .wpmega-widgetarea .widget_channel-widget {
  border: none !important;
  width: 300px;
}
#header #megaMenu .wpmega-widgetarea .widget_channel-widget #channelControls a {
  padding: 0;
  display: inline;
}
#locateChannel {
  background: #dddfe0 url(/wp-content/themes/carrington-business/img/cf-background.png) no-repeat center top;
  height: 165px;
  margin-bottom: 10px;
  
  h3 {
    color: #ff8400;
    font-family: "Helvetica","Arial", sans-serif;
    text-transform: uppercase;
    font-size: 14px;
    font-style: italic;
    margin: 0;
    padding-top: 6px;
    font-weight: bold;
  }
  #channelControlsContainer {
    background: #000000 url(/wp-content/themes/carrington-business/img/logofind.png) no-repeat 13px 34px;
    width: 275px;
    padding-top: 10px;
    padding-left: 18px;
    height: 110px;
  }
  #channelControlsContainer #channelControls {
    margin-top: 88px;
  }
  input.searchbox {
    border: 1px solid #cccccc;
    padding: 4px;
    font-family: verdana;
    font-size: 11px;
    font-weight: bold;
    color: #999;
    width: 170px;
  }
  input.button {
    margin: 0;
    border: 0;
    text-decoration: none;
    text-transform: uppercase;
    font-size: 12px;
    text-shadow: 1px 1px 1px black;
    filter: dropshadow(color=#000000, offx=1, offy=1);
    padding: 4px 8px;
    font-family: verdana;
    font-weight: bold;
    width: 81px;
    color: white;
    background-color: #d7ab55;
    background-image: url(/wp-content/themes/carrington-business/img/button-bg.png);
  }
}


/*
 * FOOTER
 *
 */
 
#footer {
  background: $darkgray image-url('footer_network_bg.gif');
  margin-top: 40px;
  
  #imo-network {
    .container {
      overflow: visible;
    }
    hr {
      border: none;
      border-bottom: 1px solid #3e3e3e;
      clear: both;
      margin: 28px 0;
    }
    h4 {
      @include inline-block;

      background: black;
      color: $gold;
      font-size: 16px;
      left: 0;
      padding: 12px 20px;
      position: absolute;
      top: -25px;
      
      .white {
        color: white;
      }
      .tail {
        @include icon-sprite(tail_black);
        
        bottom: -7px;
        left: 46px;
        position: absolute;
      }
    }
    .mag {
      margin-top: 72px;
      
      a.site-link {
        @include clearfix;
        
        &:hover h5 {
          color: $gold;
        }
      }
      .cover {
        float: left;
        margin: 0 12px 32px 0;
        vertical-align: middle;
      }
      h5 {
        @include font-stag;

        color: white;
        line-height: 1.2;
        
      }
      ul {
        clear: both;
        margin: 0 0 40px;
        padding: 0;
        
        li {
          font-size: 13px;
          line-height: 1.5;
          list-style: none;
          margin-bottom: 18px;
        }
        a {
          color: #C0C0C0;
          display: block;
          
          &:hover, &:focus {
            color: white;
          }
        }
      }
    }
    #mag-handguns h5,
    #mag-rifleshooter h5 {
      margin-top: 51px;
    }
    #mag-shooting-times h5,
    #mag-shotgun-news h5 {
      margin-top: 40px;
    }
    .extras {
      color: white;
      font-size: 13px;
      font-weight: bold;
      margin-bottom: 28px;
      
      a {
        color: white;
        
        &:hover, &:focus {
          color: $gold;
        }
      }
      form {
        float: right;
      }
    }
  }
  .stay-connected {
    background: black image-url('footer_connected_bg.gif');
    padding: 28px 0 38px;
    
    hr {
      border: none;
      border-bottom: 1px solid #2d2d2d;
      clear: both;
      margin: 30px 0;
    }
    .top, .bottom {
      @include clearfix;
    }
    h4 {
      color: $gold;
      float: left;
    }
    .connections {
      @include font-stag;
      
      float: right;
      margin: 0;
      padding: 0;
      
      li {
        @include inline-block;
        
        a {
          @include inline-block;
          
          color: white;
          margin-left: 54px;
          
          &:hover, &:focus {
            color: $gold;
          }
        }
      }
    }
    .copyright {
      color: #444;
      float: left;
      font-size: 12px;
      margin: 0;
    }
    .utility {
      color: #A1A1A1;
      float: right;
      font-size: 12px;
      font-weight: bold;
      margin: 0;
      padding: 0;
      
      a {
        color: #A1A1A1;
        margin: 0 3px;
        
        &:hover, &:focus {
          color: white;
        }
      }
    }
  }
}


/*
 * WORDPRESS STYLES
 *
 */
 
.alignnone {
  margin: 20px 0;
}
.alignleft {
  float: left;
  margin: 8px 20px 20px 0 !important;
  
  .wp-caption-text {
    padding-right: 50px;
  }
}
.alignright {
  float: right;
  margin: 8px 0 20px 20px !important;
  
  .wp-caption-text {
    padding-left: 50px;
  }
}
.aligncenter {
  display: block;
  margin: 0 auto 20px !important;
  text-align: center;
}
.wp-caption {
  border: none;
  
  img {
    margin-bottom: 1em;
  }
  .wp-caption-text {
    @include font-stag-bold-italic;
    
    color: $gunmetal;
    font-size: 18px;
    margin: 0;
    text-align: left;
  }
}


/*
 * POLLDADDY OVERRIDES
 *
 */

#sidebar .widget,
#secondary .widget {
  .pds-box {
    @include border-radius(0);

    border: none;
  }
  .pds-box-outer {
    padding: 0;
  }
  .pds-question-top {
    @include font-stag-italic;
    
    color: #A8A8A8;
    font-size: 16px;
    padding: 0;
    text-align: center;
  }
  .pds-answer {
    border: none;
    padding: 32px 0;
  }
  .pds-answer-group {
    padding: 4px 0 4px 54px;
  }
  .pds-input-label {
    width: 195px;
  }
  .pds-votebutton-outer {
    text-align: center;
  }
  .pds-vote-button {
    @include border-radius(0);
    @include inline-block;
    @include font-stag;

    background: $red image-url('button_bg.png') 0 0 repeat-x;
    border: 1px solid #AF0C10;
    color: white;
    font-size: 14px;
    height: 35px;
    line-height: 35px;
    padding: 0 32px 0 42px;
    position: relative;
    text-align: center;
    vertical-align: baseline;

    span {
      background: image-url('icon/arrow_white.png') right center no-repeat;
      padding-right: 15px;
    }
    &:hover {
      background: black;
      border-color: black;
    }
  }
  .pds-links a {
    @include font-platin;
    
    color: black;
    
    &:hover {
      color: $red;
      text-decoration: none;
    }
  }
}


/*
 * INTENSE DEBATE OVERRIDES
 *
 */
 
#main #idc-container {
  margin: 0;
  
  .idc-text,
  textarea.idc-text,
  textarea.idc-text_noresize {
    @include border-radius(0);
  }
  textarea.idc-text,
  textarea.idc-text_noresize {
    width: 98% !important;
  }
  .idc-head {
    border-top: 4px solid black;
    margin-bottom: 36px;
    position: relative;
    
    h3 {
      @include inline-block;

      background: image-url('section_title_comments.gif') 0 0 no-repeat;
      height: 51px;
      position: relative;
      top: -4px;
      width: 201px;

      #idc-commentcount_wrap, #idc-commentcount_label {
        display: none;
      }
    }
    .idc-right {
      margin-top: 86px;
      
      .idc-head_tools-login {
        float: left;
        margin-right: 5px;
      }
      .idc-share {
        
      }
    }
  }
  .idc-toolbar {
    border-bottom: 1px solid #CCC;
    margin: 24px 0;
    padding-bottom: 18px;
    text-align: left;
  }
  #idc-sortLinks p {
    font-weight: bold;
    a {
      color: $gunmetal;
      font-weight: normal;
      &:hover {
        color: $red;
      }
      &.idc-sel {
        color: $red;
        font-weight: bold;
      }
    }
    
  }
  .idc-comments {
    .idc-c .idc-c-h {
      background: none;
      
      .idc-i span {
        color: $red;
        font-size: 15px;
        font-weight: bold;
      }
    }
    .idc-c {
      border-bottom: 1px solid #CCC;
      margin: 0;
      padding: 24px 24px 42px;
    }
    .idc-c-t {
      @include font-helvetica;

      background: image-url('comments_quote_bg.png') 0 0 no-repeat;
      margin: 0 0 24px 75px;
      padding: 0 58px 0 67px;
      
      .idc-c-t-inner {
        color: #4C4C4C;
        font-size: 15px;
        line-height: 1.6;
        min-height: 40px; // so the background image doesn't get cut off on short comments
      }
    }
    .idc-thread .idc-thread .idc-c {
      background: #F7F7F7;
      padding-left: 90px;
    }
    .idc-thread .idc-thread .idc-thread {
      padding: 0 !important;

      .idc-c {
        background: #F1F1F1;
        padding-left: 156px;
      }
    }
    .idc-time {
      @include font-platin;
    }      
  }
  .idc-btn_s, .idc-btn_l {
    span {
      background: none;
      height: 0;
      width: 0;
    }
    .idc-r {
      @include font-stag;
      @include text-shadow(0 1px 0 white);
      
      background: #F2F2F2 image-url('comments_reply_bg.png') 0 0 repeat-x;
      border: 1px solid #CACACA;
      color: $gunmetal;
      font-size: 14px !important;
      height: 28px;
      line-height: 28px;
      padding: 0 12px;
      width: auto;
    }
  }
  .idc-btn_l .idc-r {
    background: $red image-url('button_bg.png') 0 0 repeat-x;
    border-color: #AF0C10;
    color: white;
    font-size: 15px !important;
    height: 40px;
    line-height: 40px;
    padding: 0 24px;
    position: relative;
    vertical-align: baseline;
    
    &:hover {
      background: black;
      border-color: black;
    }
  }
  .idc-btn_s {
    margin: 0 10px 0 142px;
  }
  .idc-c-b {
    line-height: 28px !important;
  }
  .idc-btn_l-secondary,
  .idc-new_subscribe {
    line-height: 40px !important;
  }
  .idc-new {
    h3 {
      margin: 50px 0;
      text-align: center;
    }
  }
  .idc-foot {
    border: none;
    margin-bottom: 100px;
  }
}


