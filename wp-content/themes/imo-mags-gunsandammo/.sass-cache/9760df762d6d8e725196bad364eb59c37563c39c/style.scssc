  margin: 0 auto;
  padding-right: 300px;
  position: relative;
  top: -92px;
  width: 680px;
  z-index: 100;
  
  ul.megaMenu {
    background: image-url('mega_menus_bg.png') 0 0 repeat-x;
    margin: 0;
    padding: 0;
