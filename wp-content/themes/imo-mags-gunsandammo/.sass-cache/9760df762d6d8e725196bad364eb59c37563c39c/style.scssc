  margin-bottom: 30px;
  text-align: center;
  min-height: 90px;
  width: 100%;
  position: relative;
  clear: both;
  
  .ad-left {
    position: absolute;
    left: 60px;
    width: 730px;
  }
  .ad-right {
    position: absolute;
    right: 60px;
    width: 120px;
  }
}


