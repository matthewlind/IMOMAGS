 *
 */

.entry-full {}


/*
 * LAYOUT
 * Create a layout with the following elements
 *
 */
 
#header {
  position: relative;
  width: 100%;
  z-index: 10;
}
.container {
  @include container;
  position: relative;
}
#main {
  background: white;
  margin-top: 0;
  padding: 60px 10px 0; // So the author photos can hang over the edge
  position: relative;
  z-index: 5;
  
  #masthead {
    left: 10px;
    position: absolute;
    top: 0;
  }
  #breadcrumbs {
    @include column(8);
  }
  #sidebar,
  .cfct-sidebar,
  #carrington-modules .cfct-row-ab-c .cfct-block-c {
    @include column(4,true);
    float: right;
    margin-left: 16px;
    width: 300px;
  }
  #content, .cfct-block-abc { // may need to apply to home page only
    @include column(8);
    float: left;
  }
  #secondary {
    @include column(12,true);
    
    .widget {
      float: left;
      margin-right: 16px;
      width: 316px;
      
      &:last-child {
        margin-right: 0;
      }
    }
  }
}
#footer {
  #imo-network {
    .mag {
      @include column(3);
      &.last {
        @include column(3,true);
      }
    }
  }
}

/* Carrington Module Layout */
#carrington-modules.col-abc {
  width: 100%;
  .cfct-row {
    float: left;
    margin-bottom: 32px;
    width: 100%;
  }
  .cfct-block-a,
  .cfct-block-b,
  .cfct-block-c {
    @include column(4);
  }
  .cfct-block-ab,
  .cfct-block-bc {
    @include column(8);
  }
  .cfct-block-abc {
    @include column(12);
  }
  .cfct-block-c,
  .cfct-block-bc,
  .cfct-block-abc {
    margin-right: 0;
  }
}

/* Carrington Module Layout within #content */
#content {
  .cfct-row {
    float: left;
    margin-bottom: 32px;
    width: 100%;
  }
  .cfct-block-a,
  .cfct-block-c {
    @include column(3);
  }
  .cfct-block-ab,
  .cfct-block-bc {
    @include column(5);
  }
  .cfct-block-abc {
    @include column(8);
  }
  .cfct-block-c,
  .cfct-block-bc,
  .cfct-block-abc {
    margin-right: 0;
  }
  .cfct-row-a-b-c {
    .cfct-block {
      float: left;
      margin-right: 15px;
      width: 206px;
    }
    .cfct-block-c {
      margin-right: 0;
    }
  }
}


